{
  "name": "Friends Random Episode",
  "description": "Choose a random friends episode from putlocker.is",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "http://putlocker.is/*"
  ],
  "browser_action": {
    "default_title": "Choose random episode",
    "default_popup": "popup.html"
  },
  "icons": {
	  "16": "16x16.png",
	  "48": "48x48.png",
	  "128": "128x128.png"
  }
}
