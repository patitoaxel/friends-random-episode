{
  "name": "Friends Random Episode",
  "description": "Choose a random friends episode from putlocker.is",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "tabs",
    "http://putlocker.is/*"
  ],
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "Choose random episode",
    "default_popup": "popup.html"
  }
}
